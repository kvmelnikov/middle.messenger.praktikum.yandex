{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ES2020",
        "sourceMap": true,
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strictNullChecks": true,
        "allowSyntheticDefaultImports": true
    }
}
